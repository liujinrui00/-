/**index.wxss**/
.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  padding: 20rpx;
  flex: 1;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floot_group .floor_title {
  padding: 10rpx 0;
}
.index_floor .floot_group .floor_title image {
  width: 100%;
}
.index_floor .floot_group .floot_list {
  overflow: hidden;
}
.index_floor .floot_group .floot_list navigator {
  float: left;
  width: 33.33%;
  /* 楼层后四个图片*/
  /* 2 3 两个图片*/
}
.index_floor .floot_group .floot_list navigator:nth-last-child(-n+4) {
  /*原图宽高 232 * 386 */
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floot_group .floot_list navigator:nth-child(2),
.index_floor .floot_group .floot_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floot_group .floot_list navigator image {
  width: 100%;
  height: 100%;
}
